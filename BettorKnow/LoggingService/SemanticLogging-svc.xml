<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://schemas.microsoft.com/practices/2013/entlib/semanticlogging/etw SemanticLogging-svc.xsd">
  
  <!-- Optional settings for fine tuning performance and Trace Event Session identification-->
  <traceEventService/>

  <sinks>
    <flatFileSink name="svcRuntime" fileName="SemanticETWLogs.log" >
      <sources>
        <eventSource name="BettorKnow-Web-Fixtures" level="Informational"/>
      </sources>
      <eventTextFormatter header="----------"/>
    </flatFileSink>    
  </sinks>


</configuration>
